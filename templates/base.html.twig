{# This is the base that all other views will extend #}

<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <link rel="icon" type="image/png" href="{{ asset('build/images/favicon.png') }}" />
    <title>{% block title %}Mobilean{% endblock %}</title>
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}
</head>
<body>
    <header>
        {% include '_navbar.html.twig' %}
    </header>

    <main>
    {% block body %}
    <h1>{{ pageTitle }}</h1>
    {% endblock %}
    </main>

{% block javascripts %}
    {{ encore_entry_script_tags('app') }}
{% endblock %}
    <footer>
        {% include '_footer.html.twig' %}
    </footer>
</body>
</html>
